<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="uk.ac.york.mondo.integration.server.product.serverapp"
            name="Server Product">
         <property
               name="appName"
               value="Server Product">
         </property>
      </product>
   </extension>
   <extension
         id="serverapp"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="uk.ac.york.mondo.integration.server.product.Application">
         </run>
      </application>
   </extension>

</plugin>
