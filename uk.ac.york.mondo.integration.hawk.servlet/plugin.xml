<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.equinox.http.registry.servlets">
      <servlet
            alias="/thrift/hawk/tuple"
            class="uk.ac.york.mondo.integration.hawk.servlet.servlets.HawkThriftTupleServlet">
      </servlet>
      <servlet
            alias="/thrift/hawk/compact"
            class="uk.ac.york.mondo.integration.hawk.servlet.servlets.HawkThriftCompactServlet">
      </servlet>
      <servlet
            alias="/thrift/hawk/binary"
            class="uk.ac.york.mondo.integration.hawk.servlet.servlets.HawkThriftBinaryServlet">
      </servlet>
      <servlet
            alias="/thrift/hawk/json"
            class="uk.ac.york.mondo.integration.hawk.servlet.servlets.HawkThriftJSONServlet">
      </servlet>
   </extension>
   <extension
         point="org.eclipse.equinox.http.registry.filters">
      <filter
            alias="/thrift/hawk"
            class="uk.ac.york.mondo.integration.hawk.filters.HawkAuthFilter">
      </filter>
   </extension>

</plugin>
